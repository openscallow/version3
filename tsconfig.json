    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "strict": true,
      "types": ["@sveltejs/kit"],
      "allowJs": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "paths": {
